


allocate 10 words for array


label populate-array
    let ptr be array
    stream 3 into ptr
    stream 1 into ptr
    stream 4 into ptr
    stream 1 into ptr
    stream 5 into ptr
    stream 9 into ptr
    return

label print-array
    let ptr be array

label print-array::loop
    stream num from ptr
    print num

    let delta be ptr minus array
    if delta lesser 10 goto print-array::loop

    return



label main
    call populate-array
    call print-array





